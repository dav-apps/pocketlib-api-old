(# params: state: Hash, uuid: String)
(if ((uuid == "mine") and (!(is_nil state.session))) (
	(var publishers (func get_publisher_table_objects (state.session.user_id)))

	(if (publishers.length == 0) (
		(# User is not a publisher)
		(func render_validation_errors ((list
			(hash
				(error "user_is_not_publisher")
				(status 400)
			)
		)))
	))

	(return publishers[0])
))

(return (func get_table_object (
	uuid
	nil
	"Publisher"
)))