(# params: state: Hash)
(if (is_nil state.session) (return))

(# Get the publishers of the user)
(var publisher_objects (func get_publisher_table_objects (state.session.user_id)))

(if (publisher_objects.length == 0) (
	(# User is not a publisher)
	(func render_validation_errors ((list
		(hash
			(error "user_is_not_publisher")
			(status 400)
		)
	)))
))