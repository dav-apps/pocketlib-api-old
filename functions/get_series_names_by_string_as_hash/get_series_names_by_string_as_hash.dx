(# params: series_names_string: string)
(var series_names (hash))

(if (!(is_nil series_names_string)) (
	(# Get each name of the series)
	(var uuids (series_names_string.split ","))

	(for uuid in uuids (
		(var name (func get_table_object (uuid (get_env "STORE_BOOK_SERIES_NAME_TABLE_ID"))))

		(if (!(is_nil name)) (
			(var series_names[name.properties.language] name.properties.name)
		))
	))
))

(return series_names)