(# params: categories_string: string)
(var keys (list))

(if (!(is_nil categories_string)) (
	(# Get each category)
	(var uuids (categories_string.split ","))

	(for uuid in uuids (
		(var category (func get_table_object (uuid env.CATEGORY_TABLE_ID)))

		(if (!(is_nil category)) (
			(keys.push category.properties.key)
		))
	))
))

(return keys)