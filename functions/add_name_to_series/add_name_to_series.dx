(# params: series: TableObject, uuid: string)
(# Get the names of the series)
(var names series.properties.names)

(if (is_nil names) (
	(# Add the first name to the names of the series)
	(var series.properties.names uuid)
) else (
	(# Add the uuid to the list of names, separated by comma)
	(var series.properties.names (names + "," + uuid))
))